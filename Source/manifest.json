{
	"manifest_version": 2,
	"name": "Emoji To Hieroglyphs",
	"version": "1.0",
	"description": "Replaces various emoji with Egyptian hieroglyphs.",
	"browser_action": {
	    "default_icon": "icon.png"
 	},
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

